FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      solidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Newmarks - Beta Method

solidSolver femNl;

femNlCoeffs
{
    nonLinear off;   // Not required for ParaFEM

    moveMesh yes;

    gravity 2.0;

    beta  0.25;
    delta 0.5;

    rbf no;
}

// ************************************************************************* //
